@{
    ViewData["Title"] = "Index";
}
@model List<ProductWithCategoryDto>




<table class="table table-striped">
<thead>
    <tr>
    <td>
        Id
    </td>
    <td>
        Name
    </td>
    <td>Category</td>
    </tr>
</thead>
<tbody>

    @foreach(var item in Model)
    {
        <tr>
        <td>@item.Id</td>
        <td>@item.Name</td>
        <td>@item.Category.Name</td>
        </tr>
    }
</tbody>
</table>